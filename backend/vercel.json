{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 1,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {"src": "/(.*)", "dest": "/"}
    ]
}